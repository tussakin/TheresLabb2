@model IEnumerable<Labb2Theres.Models.Teacher>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TeacherName)
            </th>
           @*  <th>
                @Html.DisplayNameFor(model => model.FkCourseId)
            </th> *@
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var teacher in Model) {
        <tr>
         
            
            <td>
                    @Html.DisplayFor(modelItem => teacher.TeacherName)
            </td>
                
            <td>
                <a asp-action="Edit" asp-route-id="@teacher.TeacherId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@teacher.TeacherId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@teacher.TeacherId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
